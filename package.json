{
	"name": "vscode-react-docgen-typescript",
	"displayName": "React Docgen TypeScript",
	"description": "Insert react component document in markdown",
  "version": "1.0.0",
  "publisher": "canyoutle",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
    "Other"
	],
	"activationEvents": [
    "onCommand:react-docgen-typescript.docgenAll",
    "onCommand:react-docgen-typescript.docgen"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
      {
				"command": "react-docgen-typescript.docgenAll",
				"title": "Docgen All: Insert all React component document"
			},
			{
				"command": "react-docgen-typescript.docgen",
				"title": "Docgen: Insert React component document"
      }
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mdast": "^3.0.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
  },
  "repository": "https://github.com/cytle/vscode-react-docgen-typescript",
	"dependencies": {
		"react-docgen-typescript": "^1.20.4",
		"react-docgen-typescript-markdown-render": "^0.0.2"
	}
}
