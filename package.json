{
  "name": "vscode-react-docgen-typescript",
  "displayName": "React Docgen TypeScript",
  "description": "Insert react component document in markdown",
  "version": "1.0.1",
  "publisher": "canyoutle",
  "icon": "https://img.alicdn.com/tfs/TB1Ly_MWhv1gK0jSZFFXXb0sXXa-1200-738.png",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-react-docgen-typescript.insertAllReactDoc",
    "onCommand:vscode-react-docgen-typescript.copyAllReactDoc",
    "onCommand:vscode-react-docgen-typescript.insertReactDoc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-react-docgen-typescript.insertReactDoc",
          "when": "editorLangId == markdown"
        },
        {
          "command": "vscode-react-docgen-typescript.insertAllReactDoc",
          "when": "editorLangId == markdown"
        },
        {
          "command": "vscode-react-docgen-typescript.copyAllReactDoc",
          "when": "editorLangId == typescriptreact"
        }
      ],
      "editor/context": [
        {
          "command": "vscode-react-docgen-typescript.insertReactDoc",
          "when": "editorLangId == markdown"
        },
        {
          "command": "vscode-react-docgen-typescript.insertAllReactDoc",
          "when": "editorLangId == markdown"
        },
        {
          "command": "vscode-react-docgen-typescript.copyAllReactDoc",
          "when": "editorLangId == typescriptreact"
        }
      ],
      "explorer/context": [
        {
          "command": "vscode-react-docgen-typescript.copyAllReactDoc"
        }
      ],
      "editor/title/context": [
        {
          "command": "vscode-react-docgen-typescript.copyAllReactDoc",
          "when": "editorLangId == typescriptreact"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-react-docgen-typescript.insertAllReactDoc",
        "title": "%extension.insertAllReactDoc.title%"
      },
      {
        "command": "vscode-react-docgen-typescript.insertReactDoc",
        "title": "%extension.insertReactDoc.title%"
      },
      {
        "command": "vscode-react-docgen-typescript.copyAllReactDoc",
        "title": "%extension.copyAllReactDoc.title%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
		"clean": "gulp clean",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mdast": "^3.0.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.49",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
		"del": "^4.1.1",
    "eslint": "^7.9.0",
		"event-stream": "^4.0.1",
    "glob": "^7.1.6",
		"gulp": "^4.0.2",
		"gulp-filter": "^5.1.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
    "mocha": "^8.1.3",
    "react": "^16.13.1",
		"run-sequence": "^2.2.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "repository": "https://github.com/cytle/vscode-react-docgen-typescript",
  "dependencies": {
    "react-docgen-typescript": "^1.20.4",
    "react-docgen-typescript-markdown-render": "~0.1.0",
    "vscode-nls": "^5.0.0"
  }
}
